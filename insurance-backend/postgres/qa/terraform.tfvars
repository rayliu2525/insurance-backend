provider_region1                                  = "us-east-1"
provider_region2                                  = "us-east-2"
name                                              = "postgresql-db"
owner                                             = "user"
environment                                       = "dev"
vpc_cidr                                          = "10.0.0.0/16"
create_subnet_group                               = true
create_subnet_group_rt                            = true
sg_protocol                                       = "tcp"

db_engine                                         = "postgres"
db_engine_version                                 = "tcp"
db_family                                         = "postgres14"
db_major_engine_version                           = "14"
instance_class                                    = "db.t4g.large"
allocated_storage                                 = 20
max_allocated_storage                             = 100
db_name                                           = "backend-postgre"
username                                          = "admin"
db_port                                           = 5432
multi_az                                          = true
maintenance_window                                = "Mon:01:00-Mon:04:00"
backup_window                                     = "04:00-07:00"
enabled_cloudwatch_logs_exports                   = ["postgresql", "upgrade"]
create_cloudwatch_log_group                       = true
backup_retention_period                           = 1
skip_final_snapshot                               = true
deletion_protection                               = false
aliases_use_name_prefix                           = true